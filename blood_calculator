from statistics import NormalDist


def interface():
    print("My Program")
    print("Options:")
    print("9 - Quit")
    keep_running = True
    while keep_running:
        choice = input("Enter your choice: ")
        if choice=='9':
            return



def input_HDL():
    HDL_input = input('Enter HDL value:')
    return int(HDL_input)



def check_HDL(HDL_number):
    if HDL_number>=60:
        return "Normal"
    elif HDL_number>=40 and HDL_number<60:
        return "Borderline Low'"
    else:
        return "Low"

interface()
HDL_number = input_HDL()
check_HDL(HDL_number)
